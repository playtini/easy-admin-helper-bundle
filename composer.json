{
    "name": "playtini/easy-admin-helper-bundle",
    "description": "EasyAdmin Helper Bundle",
    "type": "library",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "Playtini\\EasyAdminHelperBundle\\": "src/"
        }
    },
    "autoload_dev": {
        "psr-4": {
            "Playtini\\EasyAdminHelperBundle\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^8.4",
        "easycorp/easyadmin-bundle": "^4.24",
        "erusev/parsedown": "^1.7",
        "gupalo/dateutils": "^1.20",
        "gupalo/monolog-dbal-logger": "^1.7",
        "gupalo/symfony-form-transformers": "^1.3",
        "gupalo/uidgenerator": "^1.2",
        "peekabooauth/peekaboo-bundle": "^1.4",
        "spatie/yaml-front-matter": "^2.1",
        "symfony/framework-bundle": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "phpstan/phpstan": "^2.1",
        "roave/security-advisories": "dev-master"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "platform": {
            "php": "8.4"
        },
        "sort-packages": true
    }
}
